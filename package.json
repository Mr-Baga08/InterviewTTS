{
  "name": "thetruthschool-livekit",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "livekit:start": "chmod +x scripts/setup-livekit.sh && ./scripts/setup-livekit.sh",
    "livekit:stop": "docker stop livekit-server && docker rm livekit-server",
    "livekit:logs": "docker logs -f livekit-server",
    "test:voice": "tsx scripts/test-voice.ts",
    "test:agent": "tsx scripts/test-agent.ts",
    "test:all": "npm run test:voice && npm run test:agent",
    "setup:env": "cp .env.local.example .env.local",
    "setup:all": "npm run setup:env && npm run livekit:start",
    "clean": "rm -rf .next node_modules/.cache",
    "reset": "npm run clean && npm install && npm run setup:all"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.19",
    "@discordjs/opus": "^0.10.0",
    "@hookform/resolvers": "^4.1.3",
    "@livekit/components-react": "^2.9.9",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@ricky0123/vad-web": "^0.0.18",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "firebase": "^11.9.0",
    "firebase-admin": "^13.4.0",
    "livekit-client": "^2.13.4",
    "livekit-server-sdk": "^2.13.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web": "^0.0.129",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
